<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
                 version="2.0">
    <package>entity</package>
    <entity class="entity.DetailedReport" name="detailedReport">
        <table name="DETAILED_REPORT"/>
        <attributes>
            <id name="id">
                <column name="ID"/>
                <generated-value strategy="IDENTITY"/>
            </id>
            <basic name="entityField">
                <column name="COL_NAME" length="40"/>
            </basic>
            <basic name="oldData">
                <column name="OLD_DATA"/>
            </basic>
            <basic name="newData">
                <column name="NEW_DATA"/>
            </basic>
            <many-to-one name="generalReport" target-entity="GeneralReport">
                <join-column name="GENERAL_REPORT_ID" referenced-column-name="ID"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>